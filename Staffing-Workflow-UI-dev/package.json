{
  "name": "staffing-workflow-0.1",
  "version": "1.0.0",
  "description": "Staffing Workflow for Hiring Managers",
  "engines": {
    "node": ">=0.12"
  },
  "scripts": {
    "start": "NODE_ENV=production node server.js --use_strict",
    "start:server": "node server.js",
    "start:app": "concurrently --raw \"yarn run open:src\" \"yarn run lint:watch\"",
    "start:local": "NODE_ENV=local concurrently --raw \"gulp build-local\" \"yarn run start:app\" \"yarn run start:server\" ",
    "prebuild": "yarn run clean-dist && yarn run lint",
    "build": "babel-node tools/build.js",
    "build:develop-withtest": "yarn run test && gulp build-develop && yarn run build",
    "build:develop": "gulp build-develop && yarn run build",
    "build:production": "yarn run test && gulp build-production && yarn run build",
    "test": "jest",
    "test:watch": "gulp globals && node --harmony node_modules/jest/bin/jest --coverage --watchAll",
    "lint": "esw environments/webpack.config.* src tools --color; exit 0;",
    "lint:watch": "esw environments/webpack.config.* src tools --color --watch; exit 0;",
    "clean-dist": "yarn run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "open:coverage": "yarn run test && open coverage/lcov-report/index.html",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js"
  },
  "author": "HR-Staffing",
  "license": "MIT",
  "dependencies": {
    "axios": "0.17.1",
    "body-parser": "1.16.0",
    "compression": "1.6.2",
    "concurrently": "3.2.0",
    "cors": "2.8.3",
    "express": "4.14.1",
    "express-sslify": "1.2.0",
    "gulp": "3.9.1",
    "history": "3.0.0",
    "moment": "2.22.2",
    "object-assign": "4.1.1",
    "rc-time-picker": "3.3.1",
    "react-router": "3.2.0",
    "react-tabs": "2.2.2",
    "request": "2.79.0",
    "strict-transport-security": "0.2.0",
    "ux-tables": "1.6.0",
    "xml2js": "0.4.17"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "^23.6.0",
    "babel-loader": "6.2.10",
    "babel-plugin-module-resolver": "2.7.0",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.5",
    "babel-polyfill": "6.23.0",
    "babel-preset-latest": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.24.1",
    "browser-sync": "2.18.12",
    "chalk": "1.1.3",
    "classnames": "2.2.5",
    "connect-history-api-fallback": "1.5.0",
    "connected-react-router": "4.2.1",
    "css-loader": "0.28.4",
    "enzyme": "3.0.0",
    "enzyme-adapter-react-16": "1.0.0",
    "enzyme-to-json": "3.0.0",
    "eslint": "3.19.0",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-react": "7.0.1",
    "eslint-watch": "3.1.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.11.2",
    "gulp-bump": "2.6.1",
    "gulp-rename": "1.2.2",
    "gulp-replace": "0.5.4",
    "gulp-util": "3.0.8",
    "html-webpack-plugin": "2.24.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "^23.6.0",
    "jest-fetch-mock": "^1.6.6",
    "jest-junit": "^5.2.0",
    "material-ui": "0.18.1",
    "moxios": "0.4.0",
    "nock": "9.0.13",
    "node-sass": "4.5.3",
    "open": "0.0.5",
    "prompt": "1.0.0",
    "prop-types": "15.6.0",
    "raf": "3.3.2",
    "react": "16.0.0",
    "react-addons-test-utils": "15.5.1",
    "react-cookie": "1.0.5",
    "react-dom": "16.0.0",
    "react-redux": "5.0.5",
    "react-router-redux": "4.0.8",
    "react-tap-event-plugin": "2.0.1",
    "react-test-renderer": "16.0.0",
    "redux": "3.6.0",
    "redux-immutable-state-invariant": "2.0.0",
    "redux-mock-store": "1.2.3",
    "redux-thunk": "2.2.0",
    "replace": "0.3.0",
    "rimraf": "2.6.1",
    "run-sequence": "1.2.2",
    "sass-loader": "4.1.0",
    "style-loader": "0.18.2",
    "styled-components": "2.0.0",
    "url-loader": "0.5.8",
    "ux-core-styles": "2.0.0",
    "ux-navigation": "3.4.1",
    "ux-react-styleguide": "1.5.0",
    "webpack": "1.14.0",
    "webpack-dev-middleware": "1.9.0",
    "webpack-hot-middleware": "2.13.2",
    "webpack-md5-hash": "0.0.5"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": ""
  },
  "jest": {
    "automock": false,
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      "^.+\\.(gif|ttf|eot|svg|woff|woff2|ico)$": "<rootDir>/tools/fileMock.js"
    },
    "moduleDirectories": [
      "config",
      "src",
      "environments",
      "node_modules"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "raf/polyfill",
      "./tools/enzymeSetup.js",
      "./setupJest.js"
    ],
    "coveragePathIgnorePatterns": [
      "./config",
      "./constants",
      "./environments",
      "./tools"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "jest-junit": {
      "output": "./junit.xml"
    },
    "testEnvironment": "jsdom"
  }
}